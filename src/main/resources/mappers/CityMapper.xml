<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daimler.aftersales.iMappers.ICityDao">
    <delete id="truncate">
        delete from City
    </delete>

    <insert id="insertCityData" parameterType="com.daimler.aftersales.entity.CityPOJO">
        insert into City (CityID,ProvinceID,CityNameCn,CityNameEn,CityLevel,Sort)
        values (#{cityID},#{provinceID},#{cityNameCn},#{cityNameEn},#{cityLevel},#{sort})
    </insert>

</mapper>